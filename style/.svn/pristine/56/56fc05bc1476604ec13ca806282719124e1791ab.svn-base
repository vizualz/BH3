
/* ===== STRUCTURE ===== */
/*Primary site structure divs and classes.*/


body 
{
    margin-left: 5px;
    padding: 0px;
    margin: 0 0 0 0;
	font-size: 62.5%;

	
}

div.footer{font-family: verdana; font-size: 10px; color:#666666;}
div.content_body{width:30%; border:1px;}

/* ROWS */
tr.row {
padding: .6em .6em .8em;/*.2em .3em;*/
border-top: 1px solid 
white;
border-bottom: 1px solid 
#CCC;
text-shadow: 0 1px 0 
white;
height: 100%;
zoom: 1;
background: 
#EDEDED;
background: -webkit-gradient(linear,center top,center bottom,from(
#FCFCFC),to(
#EDEDED));
background: -moz-linear-gradient(top,
#FCFCFC,
#EDEDED);
filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#fffcfcfc',endColorstr='#ffededed',GradientType=0);
} 

/* MENUS */
a.menu{font-family: Trebuchet MS; font-size: 16px; color:#ffffff;}
a.menu:hover{font-family: Trebuchet MS; font-size: 16px; color:#ff9900;}

a.homemenu{font-family: Trebuchet MS; font-size: 14px; color:#333333;}
a.homemenu:hover{font-family: Trebuchet MS; font-size: 14px; font-weight:bold; background-color:#ff9900; color:#ffffff;}

a.menulinks{font-family: Trebuchet MS; font-size: 36px; color:#FFFFFF;}
a.menulinks:hover{font-family: Trebuchet MS; font-size: 36px; background-color:#ff9900; color:#ffffff;}

a.sell{font-family: Trebuchet MS; font-size: 20px; font-weight:bold; color:#99CC66;}
a.sell:hover{font-family: Trebuchet MS; font-size: 20px; font-weight:bold; color:#ffffff; background-color:#99CC66;}

a.sell2{font-family: Trebuchet MS; font-size: 18px; font-weight:bold; color:#99CC66;}
a.sell2:hover{font-family: Trebuchet MS; font-size: 18px; font-weight:bold; color:#ffffff; background-color:#99CC66;}

a.hunt2{font-family: Trebuchet MS; font-size: 18px; font-weight:bold; color:#99CC66;}
a.hunt2:hover{font-family: Trebuchet MS; font-size: 18px; font-weight:bold; color:#ffffff; background-color:#ff9900;}

a.hunt{font-family: Trebuchet MS; font-size: 20px; font-weight:bold; color:#ff9900;}
a.hunt:hover{font-family: Trebuchet MS; font-size: 20px; font-weight:bold; color:#ffffff; background-color:#ff9900;}

a.search{font-family: Trebuchet MS; font-size: 18px; font-weight:bold;  color:#ffCC00;}
a.search:hover{font-family: Trebuchet MS; font-size: 18px; font-weight:bold;  color:#000000;}

/* ===== PICTURES ===== */
div#preloaded-images {
   position: absolute;
   overflow: hidden;
   left: -9999px; 
   top: -9999px;
   height: 1px;
   width: 1px;
}

