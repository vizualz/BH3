/* Stick footer */
* {
	margin: 0;
}
html, body {
	height: 100%;
}
#main {
	min-height: 100%;
	height: auto !important;
	height: 100%;
	margin: 0 auto -150px; /* the bottom margin is the negative value of the footer's height */
}
#footer, #push {
	height: 150px; /* .push must be the same height as .footer */
	clear: both;
}

